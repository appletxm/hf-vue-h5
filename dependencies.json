    "javascript": {
      "0-@babel/polyfill": {
        "version": "7.4.3",
        "src": "node_modules/@babel/polyfill/dist/polyfill.min.js",
        "dest": "src/assets/js/polyfill.<%version%>.min.js"
      },
      "1-moment": {
        "version": "2.24.0",
        "src": "node_modules/moment/min/moment.min.js",
        "dest": "src/assets/js/moment.<%version%>.min.js"
      },
      "2-lodash": {
        "version": "4.17.15",
        "src": "node_modules/lodash/lodash.min.js",
        "dest": "src/assets/js/lodash.<%version%>.min.js"
      },
      "3-vue": {
        "version": "2.6.10",
        "src": "node_modules/vue/dist/vue.min.js",
        "dest": "src/assets/js/vue.<%version%>.min.js"
      },
      "4-vue-router": {
        "version": "3.0.0",
        "src": "node_modules/vue-router/dist/vue-router.min.js",
        "dest": "src/assets/js/vue-router.<%version%>.min.js"
      },
      "5-vuex": {
        "version": "3.1.1",
